(
  let a = Opaque 5555555t in
  let b = Opaque 2222222t in
  let c = Opaque 1t in
  switch c | 0 -> r1 | 1 -> r2
  where
    ret (a, b, c) = cont k (a, b, c)
  where
    r1 = cont ret (a, b, b)
  where
    r2 = cont ret (a, a, b)
) where
  k (a, b, c) =
    let symbol X = Block 1 (a, b, c) in
    let symbol Root = Block 33 (X) in
    cont done (Root)
